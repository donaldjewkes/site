---
const {link, image, text} = Astro.props
import { Image, Picture } from '@astrojs/image/components';
import DirHeader from "../components/DirHeader.astro";
import PhotoDirectory from '../components/PhotoDirectory.astro';
const grizz = import('/public/images/vancouver/van1.jpg')
// import testing from "../dol.png" don't like this import
---
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<title>Donald Jewkes</title>
	</head>
	<style>
		@import url('https://fonts.googleapis.com/css2?family=Space+Mono&display=swap');
	</style>

	<DirHeader id="top"></DirHeader>
	
	<div class="p-4 max-w-md ">
		<PhotoDirectory class="relative py-10" link="/photos/vancouver" image={import("/public/images/vancouver/van3.jpg")} text="Vancouver" alt="Vancouver bear."></PhotoDirectory>
		<PhotoDirectory class="relative py-1" link="/photos/sf" image={import("/public/images/sf/sfp8.jpg")} text="San Fransisco" alt="San Fransisco House."></PhotoDirectory>
		<div class=""></div>

	</div>
	



</html>
