---
const {link, image, text} = Astro.props
import { Image, Picture } from '@astrojs/image/components';
import DirHeader from "../components/DirHeader.astro";
const grizz = import('/public/images/vancouver/van1.jpg')
const bread = import("../assets/tester.jpg")
const icon = import("../assets/tester.jpg")
// import testing from "../dol.png" don't like this import
---
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<title>Donald Jewkes</title>
		<link href="/dist/output.css" rel="stylesheet">
	</head>
	<style>
		@import url('https://fonts.googleapis.com/css2?family=Space+Mono&display=swap');
	</style>

	<DirHeader id="top"></DirHeader>
	
	<div class="p-4 max-w-md">
		<div class=""></div>
		<div class="">
			<!-- <img class="relative py-1 w-92" src="/images/vancouver/van1.jpg" alt="Fishing for salmon.">
			<img class="relative py-1 w-92" src="/images/vancouver/van2.jpg" alt="An eagle in flight">
			<img class="relative py-1 w-92" src="/images/vancouver/van3.jpg" alt="A grizzly bear eating muscles on the beach.">
			<img class="relative py-1 w-92" src="/images/vancouver/van4.jpg" alt="An orca swimming."> -->


			<Image width={500} height={100} class="relative py-1 w-92" src={bread} alt="Fishing in a river."></Image>
			<Image class="relative py-1 w-92" src={icon } alt="Fishing in a river."></Image>
			<Image class="relative py-1 w-92" src={import("../assets/tester.jpg")} alt="Fishing in a river."></Image>
			<Image width={1000} class="relative py-1 w-92" src={bread} alt="A flying eagle"></Image>
			<Image width={750} class="relative py-1 w-92" src={import('/public/images/vancouver/van3.jpg')} alt="A grizzly bear eating muscles on the beach."></Image>
			<Image width={1000} class="relative py-1 w-92" src={import('/public/images/vancouver/van4.jpg')} alt="An orca swimming."></Image>

		</div>
		<a class="underline" href="#top">Top</a>
	</div>
	



</html>
